{
  "name": "lore",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "lint": "npm run lint --workspace=apps/web",
    "typecheck": "npm run typecheck --workspace=apps/web",
    "db:gen-types": "npx supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > packages/db/src/types.ts"
  },
  "devDependencies": {
    "supabase": "^2.70.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0"
  }
}
